  "license": "MIT",
  "engines": {
    "node": "0.10.x"
  },
  "keywords": [
    "webpack",
    "react",
    "starter",
    "boilerplate",
    "hot"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:dqdinh/react-webpack-starter.git"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "babel": "^4.3.0",
    "babel-core": "^4.3.0",
    "babel-loader": "^4.0.0",
    "body-parser": "^1.10.2",
    "bundle-loader": "^0.5.2",
    "chai": "^1.10.0",
    "css-loader": "^0.9.0",
    "expose-loader": "^0.6.0",
    "express": "^4.11.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.1",
    "highlight.js": "^8.4.0",
    "html-loader": "^0.2.3",
    "items-store": "^0.3.0",
    "jade": "^1.9.0",
    "jade-loader": "^0.7.1",
    "jquery": "^2.1.3",
    "jsx-loader": "^0.12.2",
    "jsxhint": "^0.8.1",
    "karma": "^0.12.31",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-coverage": "^0.2.6",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^0.3.1",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.3",
    "karma-webpack": "^1.5.0",
    "less": "^2.2.0",
    "less-loader": "^2.0.0",
    "lodash": "^3.0.1",
    "marked": "^0.3.3",
    "material-ui": "^0.6.0",
    "mocha": "^2.0.1",
    "nightwatch": "^0.5.36",
    "null-loader": "^0.1.0",
    "react-hot-loader": "^1.1.1",
    "react-proxy-loader": "^0.3.2",
    "react-tap-event-plugin": "^0.1.3",
    "selenium-standalone": "3.1.2",
    "showdown": "^0.3.1",
    "style-loader": "^0.8.3",
    "stylus": "^0.49.3",
    "stylus-loader": "^0.5.0",
    "superagent": "^0.21.0",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0",
    "when": "^3.6.4"
  },
  "dependencies": {
    "react": "^0.12.2",
    "react-router": "^0.12.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack-production.config.js --progress --profile --colors",
    "dev": "npm run hot-dev-server & npm run start-dev",
    "clean": "rm -rf ./node_modules/",
    "deps": "npm install --quiet --loglevel warn && ./node_modules/.bin/selenium-standalone install && mkdir ./build/",
    "upgrade": "./node_modules/npm-check-updates/bin/npm-check-updates -u && npm test",
    "hot-dev-server": "./node_modules/.bin/webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "test": "npm run karma & npm run lint",
    "karma": "./node_modules/.bin/karma start --single-run",
    "lint": "./node_modules/.bin/jsxhint --jsx-only app --exclude node_modules; true;",
    "selenium": "./node_modules/.bin/nightwatch -e firefox,chrome",
    "start": "node lib/server-production",
    "start-dev": "node lib/server-development"
  }
}
