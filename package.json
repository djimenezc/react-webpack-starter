{
  "name": "react-webpack-starter",
  "private": true,
  "version": "0.0.1",
  "author": "dd",
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dqdinh/react-webpack-starter.git"
  },
  "devDependencies": {
    "6to5": "2.13.3",
    "async": "^0.9.0",
    "body-parser": "^1.10.2",
    "bundle-loader": "^0.5.2",
    "chai": "^1.10.0",
    "css-loader": "^0.9.0",
    "expose-loader": "^0.6.0",
    "express": "^4.11.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.1",
    "html-loader": "^0.2.3",
    "items-store": "^0.3.0",
    "jade": "^1.9.0",
    "jsx-loader": "^0.12.2",
    "jsxhint": "^0.8.1",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-mocha": "^0.1.10",
    "karma-sourcemap-loader": "^0.3.2",
    "karma-webpack": "^1.3.1",
    "mocha": "^2.0.1",
    "nightwatch": "^0.5.36",
    "react-hot-loader": "^1.1.1",
    "react-proxy-loader": "^0.3.2",
    "selenium-standalone": "3.1.2",
    "style-loader": "^0.8.3",
    "stylus": "^0.49.3",
    "stylus-loader": "^0.5.0",
    "superagent": "^0.21.0",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.4.15",
    "webpack-dev-server": "^1.7.0",
    "when": "^3.6.4"
  },
  "dependencies": {
    "react": "^0.12.2",
    "react-router": "^0.11.6"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/",
    "deps": "npm install --quiet --loglevel warn && ./node_modules/.bin/selenium-standalone install",
    "upgrade": "./node_modules/npm-check-updates/bin/npm-check-updates -u && npm test",
    "jade": "./node_modules/.bin/jade --watch ./html/*.jade --pretty --out ./build",
    "stylus": "./node_modules/.bin/stylus --watch ./styles/*.styl --out ./build",
    "selenium": "./node_modules/.bin/nightwatch -e firefox,chrome",
    "webpack": "NODE_ENV=development ./node_modules/.bin/webpack ./app/App.jsx build/main.js --config webpack.config.js --progress --colors --watch",
    "webpack-dev-server": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server ./app/App.jsx --content-base build/ --hot --config webpack.config.js --progress --colors --watch",
    "build": "npm run webpack & npm run jade & npm run stylus",
    "serve": "npm run webpack-dev-server & npm run jade & npm run stylus",
    "lint": "./node_modules/.bin/jsxhint --jsx-only app --exclude node_modules; true;",
    "karma": "./node_modules/.bin/karma start --single-run",
    "test": "npm run karma & npm run lint",
    "jade-prod": "./node_modules/.bin/jade --watch ./html/*.jade --pretty --out ./dist",
    "stylus-prod": "./node_modules/.bin/stylus --watch ./styles/*.styl --out ./dist"
  }
}
